.row
  %h1.col-sm-offset-4.col-sm-4.text-center History
  -if signed_in?
    .btn-group.col-sm-offset-1.col-sm-2
      %button.btn.btn-success.dropdown-toggle(data-toggle="dropdown")<
        =@current_user.name
        %span.caret
      %ul.dropdown-menu(role="menu")
        %li
          =link_to "Log out", session_path(@current_user), method: :delete
-unless signed_in?
  =render "shared/form_signin"
#container_calendar